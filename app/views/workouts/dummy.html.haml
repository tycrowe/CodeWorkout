%button.btn.btn-large{href: "#kicModal", role: "button", data: {toggle: "modal"}} Launch Dummy Modal
#kicModal.modal.fade
  .modal-content
    .modal-header
      %button.close{data:{dismiss: "modal"}} &times;
      %h4.modal-title Confirmation
    .modal-body
      = render @workouts
      %p Minsum Dinsum
    .modal-footer
      %button.btn{data:{dismiss: "modal"}} Close
      %button.btn Save changes